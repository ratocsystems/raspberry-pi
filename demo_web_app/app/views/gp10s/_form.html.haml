= form_for @gp10 do |f|
  - if @gp10.errors.any?
    #error_explanation
      %h2= "#{pluralize(@gp10.errors.count, "error")} prohibited this gp10 from being saved:"
      %ul
        - @gp10.errors.full_messages.each do |message|
          %li= message

  .field
    = f.label :di
    = f.number_field :di
  .field
    = f.label :machine
    = f.text_field :machine
  .field
    = f.label :date
    = f.datetime_select :date
  .field
    = f.label :beginning
    = f.datetime_select :beginning
  .actions
    = f.submit 'Save'
